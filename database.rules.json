{
  "rules": {
    "users": {
      ".read": "auth !== null",
      ".write": "auth !== null",
      "$user_id": {
        ".write": "auth === $user_id",
        "rooms": {
          ".read": "auth === $user_id"
        },
        "channels": {
          ".read": "auth === $user_id"
        },
        "authentications": {
          ".read": "auth === $user_id"
        }
      }
    },
    "rooms": {
      ".read": "auth !== null",
      ".write": "auth !== null"
    },
    "channels": {
      ".read": "auth !== null",
      ".write": "auth !== null",
      "$room_id": {
        ".read": "root.child('users/'+auth.uid+'/rooms/'+$room_id).exists()",
        ".write": "root.child('users/'+auth.uid+'/rooms/'+$room_id).exists()",
        "$channel_id": {
          ".read": "root.child('users/'+auth.uid+'/channels/'+$channel_id).exists()",
          ".write": "root.child('users/'+auth.uid+'/channels/'+$channel_id).exists()"
        }
